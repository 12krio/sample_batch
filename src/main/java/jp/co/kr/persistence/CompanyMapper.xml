    SUM(CASE WHEN ORG_TYPE = '3' THEN CAPITAL ELSE 0 END) AS '3',  
    SUM(CASE WHEN ORG_TYPE = '4' THEN CAPITAL ELSE 0 END) AS '4',  
    COUNT(*) AS 'total' 
FROM COMPANY 
GROUP BY BIZ_TYPE  
WITH ROLLUP;
